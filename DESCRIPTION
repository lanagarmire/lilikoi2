Package: lilikoi
Type: Package
Title: Metabolomics Personalized Pathway Analysis Tool
Version: 2.0.3
Authors@R: c(
 person("Xinying", "Fang", email = "xinyingf@umich.edu", role = c("aut")),
 person("Yu", "Liu", email = "uiluy@umich.edu", role = "aut"),
